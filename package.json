{
  "name": "sixempress",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "production": "docker compose -f infrastructure/docker-compose.prod.yml -p sixempress up --build --force-recreate --remove-orphans -d",
    "_d-prod": "docker compose -f infrastructure/docker-compose.prod.yml -p sixempress",
    "_d-test": "yarn _d-prod -f infrastructure/docker-compose.test.yml -p sixempress",
    "_d-dev": "yarn _d-test -f infrastructure/docker-compose.dev.yml -p sixempress",
    "_d-dev-wp": "yarn _d-dev -f infrastructure/docker-compose.dev-wp.yml -p sixempress",
    "d-all-files": "docker compose $(bash -c 'dest=\"\"; for i in infrastructure/docker-compose.*; do dest+=\" -f $i\"; done; echo $dest') -p sixempress",
    "d-prod": "yarn _d-prod up --build --force-recreate",
    "d-test": "yarn _d-test up --build --force-recreate",
    "d-dev": "yarn _d-dev up --build --force-recreate",
    "d-dev-wp": "yarn _d-dev-wp up --build --force-recreate",
    "d-kill": "yarn d-all-files kill && yarn wp-env stop",
    "d-down": "yarn d-all-files down && yarn wp-env stop",
    "bbb-test": "echo \"yarn _d-test build && yarn _d-test up -d && yarn run _d-test exec -it backend-big-bang yarn nx test core-backend-big-bang --skip-nx-cache --runInBand\"",
    "bbb-dev": "echo \"yarn _d-dev build && yarn _d-dev up -d && yarn run _d-dev exec -it backend-big-bang yarn nx test core-backend-big-bang --skip-nx-cache --runInBand\"",
    "x": "yarn _d-dev build && yarn _d-dev run --service-ports -it command-runner",
    "protoc": "npx protoc --ts_out ./libs/contracts/src/dist/ --proto_path . ./libs/contracts/src/**/*.proto && node tools/scripts/protoc-improved-types-wa.js ./libs/contracts/src/dist/libs/contracts/src",
    "build-all-modules": "export NODE_ENV=production; nx run-many --target=build --prod",
    "build-all": "yarn _compile-all-command && yarn _pkg_all-command",
    "compile-all": "yarn x yarn _compile-all-command",
    "_compile-all-command": "export NODE_ENV=production; nx run-many --target=build --projects=modules-sixempress-raw-files-backend --prod",
    "_pkg_all-command": "export NODE_ENV=production; nx run-many --target=pkg --projects=modules-sixempress-raw-files-backend --prod --parallel=1",
    "_pkg": "export PKG_CACHE_PATH=/code/node_modules/.cache/pkg; mkdir -p /code/node_modules/.cache/pkg; pkg -C GZip -t node16-alpine-x64 --public",
    "postinstall": "patch-package"
  },
  "private": true,
